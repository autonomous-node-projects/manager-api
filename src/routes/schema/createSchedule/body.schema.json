{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "https://example.com/object1619738137.json", 
	"title": "Array of schedules", 
	"type": "array",
	"default": [],
	"items":{
		"$id": "#root/items", 
		"title": "Schedule object", 
		"type": "object",
		"required": [
			"scriptName",
			"every"
		],
		"properties": {
			"scriptName": {
				"$id": "#root/items/scriptName", 
				"title": "Name of script to be scheduled", 
				"type": "string",
				"default": "",
				"examples": [
					"start"
				],
				"pattern": "^.*$"
			},
			"every": {
				"$id": "#root/items/every", 
				"title": "Interval object", 
				"type": "object",
				"required": [
					"value",
					"timeType"
				],
				"properties": {
					"value": {
						"$id": "#root/items/every/value", 
						"title": "Amount of time used as interval", 
						"type": "integer",
						"examples": [
							2
						],
						"default": 0
					},
					"timeType": {
						"$id": "#root/items/every/timeType", 
						"title": "Timetype - only: hours, minutes, seconds", 
						"type": "string",
						"default": "",
						"examples": [
							"hours", "hour", "minutes"
						],
						"pattern": "^.*$"
					}
				}
			}
,
			"exitAfter": {
				"$id": "#root/items/exitAfter", 
				"title": "Exit after occurances", 
				"type": "integer",
				"examples": [
					2
				],
				"default": 0
			}
		}
	}

}